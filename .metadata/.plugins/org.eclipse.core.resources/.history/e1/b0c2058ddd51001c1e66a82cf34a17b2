
public class TestaVariaveis {
	
	public static void main(String[] args){
		 System.out.println("Olá Novo Teste!");
		 
		 int idade;
		 idade = 37;
		 
		 System.out.println(idade);
		 
		 idade = 30 + 10;
		 
		 System.out.println(idade);
		 
		 
		 idade = 7 * 5 +2;
		 
		 System.out.println("A idade é " idade);
	}
}
